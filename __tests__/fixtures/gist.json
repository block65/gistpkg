      "raw_url": "https://gist.githubusercontent.com/maxholman/80979ac8e36fbb40aba44d7115cbf600/raw/29fc08cb60c5106e9a91de3daa76601cde7f401f/readdir.ts",
      "size": 343,
      "truncated": false,
      "content": "import { readdirSync, statSync } from 'fs';\nimport { join } from 'path';\n\nfunction recursiveReaddirSync(dir: string): string[] {\n  const files = readdirSync(dir);\n  return files.flatMap((file) => {\n    const path = join(dir, file);\n    if (statSync(path).isDirectory()) {\n      return recursiveReaddirSync(path);\n    }\n    return path;\n  });\n}"
    }
